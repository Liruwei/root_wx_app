/* pages/my/about.wxss */
page {
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #f6f9f9;
}

.top {
    height: 400rpx;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #ffffff;
    margin-bottom: 40rpx;
}

.logo {
    width: 300rpx;
    height: 300rpx;
}

.row {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding: 0 20rpx 0 20rpx;
    height: 88rpx;
    box-sizing: border-box;
    background-color: #ffffff;
    margin-bottom: 0.5px;
    width: 100%;
}

.name {
    font-size: 24rpx;
    color: #303232;
}

.value {
    font-size: 24rpx;
    color: #999999;
}