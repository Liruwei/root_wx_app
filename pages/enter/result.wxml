<!--pages/enter/result.wxml-->
<view class="Result">
	<view class="Icon">
		<icon type="success" size="60" color="#cc9c00"></icon>
	</view>
		<view class="Title">入驻成功</view>
	<view>
		<view class="Item">
			<view class="Name">店铺名称</view>
			<view class="Value">{{project.name}}</view>
		</view>
		<view class="Item">
			<view class="Name">创建时间</view>
			<view class="Value">{{project.create_time}}</view>
		</view>
		<view class="Item">
			<view class="Name">联系方式</view>
			<view class="Value">{{project.phone}}</view>
		</view>
		<view class="Item">
			<view class="Name">配送功能</view>
			<view class="Value">{{project.open_send === 1 ? '打开' : '关闭'}}</view>
		</view>
	</view>
	<view class="ToHome" catchtap="toHomeTap">前往店铺</view>
</view>