/* pages/enter/index.wxss */
page {
    font-size: 28rpx;
    color: #cc9c00;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 40rpx;
    background-color: #f2f4f5;
}

.EnterInvite {
    margin-bottom: 40rpx;
    width: calc(100vw - 80rpx);
    min-width: calc(100vw - 80rpx);
    height: 200rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 40rpx;
    border-radius: 10rpx;
    flex-direction: row;
    padding: 0;
    font-weight: normal;
    background-color: #fff;
    color: #cc9c00;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
}

.EnterMe {
    width: calc(100vw - 80rpx);
    height: 200rpx;
    display: flex;
    align-items: center;
    flex-direction: row;
    justify-content: center;
    background-color: #fff;
    font-size: 40rpx;
    border-radius: 10rpx;
    padding: 0;
    font-weight: normal;
    background-color: #fff;
    color: #cc9c00;
    margin: 0;
}